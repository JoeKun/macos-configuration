[user]
	name = "Foo"
	email = "foo@domain.tld"
[core]
	abbrev = 12
	excludesfile = /Users/foo/.gitignore_global
	pager = /opt/local/share/git-core/contrib/diff-highlight/diff-highlight | vimpager
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[push]
	default = simple
[apply]
	whitespace = nowarn
